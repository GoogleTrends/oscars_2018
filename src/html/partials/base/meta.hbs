{{! See variable definitions in template-data/info.json }}
<title>{{copy.hed}}</title>
<meta charset='utf-8'>
<meta name='viewport' content='width=device-width, initial-scale=1'>
<meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'>
<meta name='description' content='{{copy.description}}' />
<meta name='news_keywords' content='{{copy.keywords}}' />

<meta property='og:title' content='{{copy.hed}}' />
<meta property='og:site_name' content='Google Trends' />
<meta property='og:url' content='{{copy.url}}' />
<meta property='og:description' content='{{copy.description}}' />
<meta property='og:type' content='article' />
<meta property='og:locale' content='en_US' />

<meta property='og:image' content='{{copy.url}}/assets/social/og-img.jpg' />
<meta property='og:image:type' content='image/jpeg' />
<meta property='og:image:width' content='1200' />
<meta property='og:image:height' content='600' />

<meta name='twitter:card' content='summary_large_image' />
<meta name='twitter:site' content='https://google.com/trends' />
<meta name='twitter:creator' content='{{copy.twitter}}' />
<meta name='twitter:title' content='{{copy.hed}}' />
<meta name='twitter:description' content='{{copy.description}}' />
<meta name='twitter:image:src' content='{{copy.url}}/assets/social/og-img.jpg' />

<link rel='canonical' href='{{copy.url}}' />